
#include "inc/Debug.hpp"
#include "inc/Info.hpp"
#include "inc/Error.hpp"
#include "inc/Warning.hpp"

#include <thread>

using namespace logging_level;


int main()
{
    const std::string text = "Hello world!";

    std::thread t1(debug, text);
    std::thread t2(info, text);
    std::thread t3(warning, text);
    std::thread t4(error, text);
    t1.join();
    t2.join();
    t3.join();
    t4.join();

}
